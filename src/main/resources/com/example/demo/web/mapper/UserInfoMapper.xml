<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.web.mapper.UserInfoMapper">
    
    <!-- 1件検索 -->
    <select id="findByUsername" resultType="com.example.demo.entity.UserInfo">
        select
            username,
            roles,
            password,
            email,
            regist_date,
            version_no
        from
            user_info
        where
            username = #{username}
    </select>

</mapper>